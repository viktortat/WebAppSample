{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=PizzaWebDb;ConnectRetryCount=0;Trusted_Connection=True;MultipleActiveResultSets=true",
    "DefaultConnectionLite": "DataSource=pizzas.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MiddlewareSettings": {
    "UseTimeLoggingMiddleware": "true",
    "UseCultureMiddleware": "true",
    "UseIntentionalDelayMiddleware": "true",
    "UseErrorHandlingMiddleware": "true",
    "UseCorrelationMiddleware": "true"
  }
}